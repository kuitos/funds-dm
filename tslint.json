{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "indent": [
      "tabs"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "no-console": false,
    "arrow-parens": [
      "ban-single-arg-parens"
    ],
    "object-literal-sort-keys": false,
    "interface-name": false,
    "max-line-length": [
      true,
      140
    ]
  },
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ]
}
